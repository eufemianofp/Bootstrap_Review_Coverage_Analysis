> # 2. PERFORM BOOTSTRAP ANALYSIS
> ##################################
> 
> ## Define estimator function
> estimator_fn <- function(x, index, alpha){
+   
+   theta_hat <- mean(x)  # estimator value
+   
+   x_star <- x[index]  # bootstrap resamples
+   mu_hat <- mean(x_star)  # bootstrap estimates
+   
+   boot2 <- boot(data = x_star, statistic = function(x_star, ind) { return(mean(x_star[ind])) }, R = B)
+   ci.boot2 <- boot.ci(boot.out = boot2, conf = 1-alpha, type = "perc")
+   indicator <- ci.boot2$percent[4] < theta_hat && ci.boot2$percent[5] > theta_hat  # does CI for "parameter" theta_hat contain theta_hat?
+   
+   return(indicator)
+ }
