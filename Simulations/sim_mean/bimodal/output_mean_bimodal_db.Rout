> ## Simulation design
> interval.types <- c("percent_db")
> ns <- c(10, 100, 1000)
> alpha_goal <- 0.05
> alphas_step <- 0.02
> steps <- -2:2
> alphas <- alpha_goal + alphas_step * steps
> coverage_goal <- 1 - alpha_goal
> emp_coverages <- numeric(length = length(alphas))  # vector of empirical coverages
> M <- 200  # number of CIs to compute empirical coverage for a given alpha
> B <- 1.5e4 -1  # bootstrap replicates to compute CIs
> R <- 500  # number of simulations for coverage analysis 
> 
> ## Population parameters
> mu <- 0  # normal mean
> sd <- 1  # normal sd
> # mu <- 1  # exponential and lognormal mean
> # sd <- 0.4  # lognormal sd
> dist <- 3*sd  # distance between std normals for bimodal distribution
> 
> 
> ##################################
> # 2. PERFORM BOOTSTRAP ANALYSIS
> ##################################
> 
> ## Define estimator function
> estimator_fn <- function(x, index, alpha){
+   
+   theta_hat <- mean(x)  # estimator value
+   
+   x_star <- x[index]  # bootstrap resamples
+   mu_hat <- mean(x_star)  # bootstrap estimates
+   
+   boot2 <- boot(data = x_star, statistic = function(x_star, ind) { return(mean(x_star[ind])) }, R = B)
+   ci.boot2 <- boot.ci(boot.out = boot2, conf = 1-alpha, type = "perc")
+   indicator <- ci.boot2$percent[4] < theta_hat && ci.boot2$percent[5] > theta_hat  # does CI for "parameter" theta_hat contain theta_hat?
+   
+   return(indicator)
+ }
